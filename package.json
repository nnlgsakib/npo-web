{
  "name": "npo-api-skeleton",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.ts",
     "build": "bun build --compile  ./src/index.ts  --target node --outfile dist/npo",
    "start": "bun src/index.ts",
    "start:node": "node dist/index.js",
    "dev:node": "node dist/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "level": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "pino": "^9.12.0",
    "pino-pretty": "^11.2.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.30",
    "typescript": "^5.4.5"
  }
}
